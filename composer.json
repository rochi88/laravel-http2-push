{
    "name": "rochi88/laravel-http2-push",
    "description": "Easily utilise the HTTP/2 push Feature in your Laravel App",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Reimann",
            "email": "marcoreimann@outlook.de",
            "homepage": "https://mrcrmn.com"
        }
    ],
    "homepage": "https://github.com/rochi88/http2push",
    "keywords": ["Laravel", "Http2Push"],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^4|^5|^6|^7",
        "sempro/phpunit-pretty-print": "^1.4.0"
    },
    "autoload": {
        "psr-4": {
            "rochi88\\Http2Push\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "rochi88\\Http2Push\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rochi88\\Http2Push\\Http2PushServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
